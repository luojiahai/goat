proc insertion_sort()
    int length;
    int collection[3];
    int i;
    int tmp;
begin
    length := 3;
    collection[0] := 3;
    collection[1] := 6;
    collection[2] := 2;
    write "unsorted: ";
    write collection[0];
    write collection[1];
    write collection[2];
    write "\n";

    i := 1;
    while i < length do
        while i > 0 && collection[i - 1] > collection[i] do
            tmp := collection[i];
            collection[i] := collection [i + 1];
            collection [i] := tmp;
            i := i - 1;
        od
        i := i + 1;
    od

    write "sorted: ";
    write collection[0];
    write collection[1];
    write collection[2];
    write "\n";
end

proc main()
begin
    write "Hello, this is an insertion sort.\n";
    call insertion_sort();
end